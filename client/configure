NAME="client"
RM="rm -f"
QMAKE="qmake-qt4"
MAKE="make"

# QMAKE SETTING
export QTDIR="/usr/local/share/qt4"
export QMAKESPEC=$QTDIR"/mkspecs/freebsd-g++"
# !QMAKE SETTING

$RM $NAME;
$QMAKE && \
$MAKE;
